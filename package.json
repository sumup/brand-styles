{
  "name": "brand-styles",
  "version": "2.0.0",
  "description": "The SumUp brand styles Sass library.",
  "scripts": {
    "lint": "node node_modules/sass-lint/bin/sass-lint.js '*.scss' --verbose --no-exit",
    "lint:watch": "npm run lint; onchange '*.scss' -- npm run lint",
    "test": "node node_modules/sass-lint/bin/sass-lint.js '*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/brand-styles.git"
  },
  "keywords": [
    "Sass",
    "Library"
  ],
  "bugs": {
    "url": "https://github.com/sumup/brand-styles/issues"
  },
  "homepage": "https://github.com/sumup/brand-styles#readme",
  "dependencies": {
    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.0.0",
    "normalize.css": "^7.0.0",
    "sass-lint": "^1.10.2"
  },
  "devDependencies": {
    "onchange": "^2.5.0"
  }
}
